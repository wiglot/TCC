\begin_layout BeginFrame
Farthest
\end_layout

\begin_layout Itemize
Escolha os dois pontos mais afastados(
\begin_inset Formula $i\, e\, j\in I$
\end_inset

).
 Aloque-os como primeiros centros (
\begin_inset Formula $C=\{i,j\}$
\end_inset

);
\end_layout

\begin_layout Itemize
Escolha um ponto 
\begin_inset Formula $k\in I$
\end_inset

 de modo que: 
\begin_inset Formula \[
\prod_{j\in C}d_{kj}=Max\;\prod_{j\in C,\, k\in I-C}d_{kj}\]

\end_inset

e aloque como novo centro ( 
\begin_inset Formula $C=C\cup k$
\end_inset

 ).
 Repita até 
\begin_inset Formula $|C|=P$
\end_inset

;
\end_layout

\begin_layout Itemize
Aloque os pontos na ordem de proximidade com os centros;
\end_layout

