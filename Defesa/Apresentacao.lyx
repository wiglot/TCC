\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
Assoc_{ij} &  & igual\, a\,1\, se\, o\, ponto\, i\, está\, associado\, ao\, agrupamento\, j;\\
Dist_{ij} &  & distância\, entre\, os\, pontos\, i\, e\, j;\\
p &  & número\, de\, agrupamentos;\\
Med_{i} &  & igual\, a\,1\, se\, o\, ponto\, i\,é\, a\, mediana\, de\, um\, agrupamento;\\
Dem_{i} &  & Demanda\, do\, ponto\, i;\\
Cap_{j} &  & Capacidade\, do\, agrupamento\, j;\end{eqnarray*}

\end_inset


\begin_inset space ~
\end_inset


\end_layout

\begin_layout BeginFrame
Modelo matemático 
\end_layout

\begin_layout Block
\begin_inset space ~
\end_inset


\begin_inset Formula \begin{eqnarray}
Min &  & \sum_{j\in P}\;\sum_{i\in I}Dist_{ij}Assoc_{ij}\\
Sujeito\;à:\;\;\nonumber \\
\sum_{j\in p}Assoc_{ij} & = & 1,\;\forall i\in I\\
\sum_{j\in I}Med_{j} & = & p\\
Assoc_{ij} & \leq & Med_{j,\;}\forall i\in I,\:\forall j\in P\\
\sum_{i\in I}Dem_{i}Assoc_{ij} & \leq & Cap_{j},\;\forall j\in P\end{eqnarray}

\end_inset


\end_layout

\begin_layout Block
\begin_inset Formula \begin{equation}
Onde:\;\; Assoc_{ij},\: Med_{j}\in\{0,1\},\;\forall i\in I,\;\forall j\in P\end{equation}

\end_inset


\end_layout

